{
  "name": "sws-backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "f.riva@sotefin.ch",
  "license": "MIT",
  "scripts": {
    "all": "concurrently --kill-others \"yarn api\" \"yarn dev\"",
    "api": "nodemon lib/server.js --exec babel-node --presets es2015,stage-2",
    "dev": "nodemon lib/main.js --exec babel-node --presets es2015,stage-2",
    "gen": "babel-node lib/aps/nyu/generator.js --exec babel-node --presets es2015,stage-2",
    "build": "rm -r dist && babel lib -d dist",
    "start": "NODE_ENV=production node dist/ws.js"
  },
  "dependencies": {
    "async": "^2.6.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "moment": "^2.22.2",
    "mongoose": "^5.3.12",
    "node-snap7": "^1.0.1",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "standard": "^12.0.1",
    "winston": "^3.1.0"
  }
}
